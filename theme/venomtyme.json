            "settings": {
                "foreground": "#FAE3B0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "this, super, self, etc.",
            "scope": [
                "variable.other.object",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#F28FAD",
                "fontStyle": "italic"
            }
        },
        {
            "name": "this, super, self, etc.",
